{
  "name": "vue-dnd-forms-monorepo",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev -w packages/app",
    "build": "npm run build --workspaces",
    "build:app": "npm run build -w packages/app",
    "build:cli": "npm run build -w packages/cli",
    "build:form-builder": "npm run build -w packages/form-builder",
    "test": "npm run test -w packages/app",
    "test-e2e": "npm run test-e2e -w packages/app",
    "test:browser": "npm run test:browser -w packages/app",
    "publish:form-builder": "npm publish -w packages/form-builder --access public",
    "publish:cli": "npm publish -w packages/cli --access public"
  },
  "devDependencies": {
    "prettier": "3.5.3",
    "typescript": "~5.7.2"
  }
}